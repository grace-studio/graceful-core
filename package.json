{
  "name": "@grace-studio/graceful-next",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    "./utils": {
      "import": "./lib/esm/utils/index.js",
      "require": "./lib/cjs/utils/index.js"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "lib/types/utils/index.d.ts"
      ],
      "components": [
        "lib/types/components/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf lib && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:types": "tsc --declarationDir lib/types --declaration --emitDeclarationOnly",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm test && npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-studio/graceful-next-core.git"
  },
  "keywords": [],
  "author": "Grace Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-studio/graceful-next-core/issues"
  },
  "homepage": "https://github.com/grace-studio/graceful-next-core#readme",
  "peerDependencies": {
    "next": "13"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "@types/react-dom": "18.0.11",
    "@types/react": "18.0.28",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "next": "13.2.3",
    "prettier-eslint": "15.0.1",
    "prettier": "2.8.4",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
