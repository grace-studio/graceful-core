{
  "name": "@grace-studio/graceful-next",
  "version": "0.0.4",
  "description": "",
  "type": "module",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    "./utils": {
      "import": "./lib/esm/utils/index.js",
      "require": "./lib/cjs/utils/index.js"
    },
    "./components": {
      "import": "./lib/esm/components/index.js",
      "require": "./lib/cjs/components/index.js"
    },
    "./types": {
      "import": "./lib/esm/types/index.js",
      "require": "./lib/cjs/types/index.js"
    },
    "./strapi": {
      "import": "./lib/esm/strapi/index.js",
      "require": "./lib/cjs/strapi/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "lib/types/utils/index.d.ts"
      ],
      "components": [
        "lib/types/components/index.d.ts"
      ],
      "types": [
        "lib/types/types/index.d.ts"
      ],
      "strapi": [
        "lib/types/strapi/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf lib && yarn build:esm && yarn build:cjs && yarn build:types",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:types": "tsc --declarationDir lib/types --declaration --emitDeclarationOnly",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "prepublishOnly": "yarn test && yarn lint && yarn build",
    "preversion": "yarn test && yarn lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grace-studio/graceful-next-core.git"
  },
  "keywords": [],
  "author": "Grace Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grace-studio/graceful-next-core/issues"
  },
  "homepage": "https://github.com/grace-studio/graceful-next-core#readme",
  "peerDependencies": {
    "next": "13",
    "tailwindcss": "3",
    "classnames": "2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "classnames": "2.3.2",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "next": "13.2.4",
    "prettier": "2.8.4",
    "prettier-eslint": "15.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.2"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
